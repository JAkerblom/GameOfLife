<Window x:Class="GameOfLife.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameOfLife.GUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
  <Grid>
    <Grid HorizontalAlignment="Left" Height="301" Margin="472,10,-292.333,0" VerticalAlignment="Top" Width="339"/>
    <Button x:Name="button" Content="Start Game" HorizontalAlignment="Left" Margin="10,275,0,0" VerticalAlignment="Top" Width="147" Click="button_Click" Height="28"/>
    <Slider x:Name="slider" HorizontalAlignment="Left" Margin="10,189,0,0" VerticalAlignment="Top" Width="147"/>
    <Label x:Name="label" Content="Speed" HorizontalAlignment="Left" Margin="10,159,0,0" VerticalAlignment="Top" Width="147"/>
    <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="10,71,0,0" VerticalAlignment="Top" Width="147" SelectionChanged="comboBox_SelectionChanged"/>
    <Label x:Name="label1" Content="Pre-sets" HorizontalAlignment="Left" Margin="10,45,0,0" VerticalAlignment="Top" Width="155"/>
    <Button x:Name="button1" Content="Randomize" HorizontalAlignment="Left" Margin="88,121,0,0" VerticalAlignment="Top" Width="69" Click="button1_Click"/>

  </Grid>
</Window>
